<div class="overlay print-list multi-select">
  <div class="header">
    <h2>
      <%= t 'actions.print' %>
    </h2>
    <div class="submit button">
      <%= t 'actions.print' %>
    </div>
    <div class="abort button">
      <%= t 'actions.abort' %>
    </div>
  </div>
  <div class="select">
    <%= print_list_dialog_for(@item_type, params[:list_id]) do |options| %>
      <% options.in_groups(3, false) do |group| %>
        <ul class="col">
          <% group.each do |name, attr, human| %>
            <li>
              <label>
                <%= check_box_tag(name, attr, true, id: nil) %> <%= human %>
              </label>
            </li>
          <% end %>
        </ul>
      <% end %>
    <% end %>
  </div>
</div>
