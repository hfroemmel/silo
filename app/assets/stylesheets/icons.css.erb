/**
 * icons.css
 *
 * Defines all icons.
 */
.icon-delete {
  background: transparent url('<%= asset_data_uri 'icon_delete.png' %>') center center no-repeat;
}

.icon-edit {
  background: transparent url('<%= asset_data_uri 'icon_edit.png' %>') left center no-repeat;
}

.icon-expert-add {
  background: transparent url('<%= asset_data_uri 'icon_expert_add.png' %>') center top no-repeat;
}

.icon-list {
  background: transparent url('<%= asset_data_uri 'icon_list.png' %>') center top no-repeat;
}

.icon-logout {
  background: transparent url('<%= asset_data_uri 'icon_logout.png' %>') right center no-repeat;
}

.icon-print {
  background: transparent url('<%= asset_data_uri 'icon_print.png' %>') left center no-repeat;
}

.icon-user-add {
  background: transparent url('<%= asset_data_uri 'icon_user_add.png' %>') center top no-repeat;
}

.logo {
  background: transparent url('<%= asset_data_uri 'logo.png' %>') left center no-repeat;
}
